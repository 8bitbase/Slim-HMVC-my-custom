	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>{{ title }}</title>
	<meta name="viewport" content="width=device-width, initial-scale=1">

	{% block meta %}
        {% include 'general/view/meta.twig' %}
    {% endblock %}

	
	{% if isProduction() %}		
		<link rel="stylesheet" href="{{ base_url() }}/assets/css-min/style.min.css">
	{% else %}
		<link rel="stylesheet" href="{{ base_url() }}/assets/css-develop/style.css">
	{% endif %}

</head>
